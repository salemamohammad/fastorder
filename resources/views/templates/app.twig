	<link rel="stylesheet" href="../../../css/product.css">
	<!-- <link rel="stylesheet" href="../../../public/css/product.css"> -->
</head>



<body>

	{% include 'templates/commons/navigation.twig'%}
	<div class="container">
		
		{% block content %} 
			<div class="container-fluid px-5 py-5 mx-auto">
			    <div class="row d-flex mb-4 px-3">
			        <h5 class="pr-4 bread-crumb"> Home </h5>
			        <h5 class="pr-4"> > </h5>
			        <h5 class="text-warning bread-crumb">My Products</h5>
			    </div>
			    <div class="row justify-content-between px-4" id="product_div">
			    </div>
			</div>

		{% endblock %}
	</div>



	<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>	
	<!-- <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"></script> -->
